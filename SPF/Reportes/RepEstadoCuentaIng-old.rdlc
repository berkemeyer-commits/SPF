  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select lpr.ClienteID, lpr.ClienteNombre, lpr.ClienteIdiomaID, CASE WHEN lpr.ClienteIDiomaID = 2 THEN 'Trámites Agrupados (*)' ELSE 'Grouped Procedures (*)' END AS Tramite, NULL AS CodigoRESI, lpr.FechaDoc, lpr.FechaDocAsDate, lpr.NroDoc, SUM(lpr.Saldo) AS Saldo, lpr.Moneda, GETDATE() AS FechaGeneracion, 0 AS PresupuestoID, 0 AS TramiteID, lpr.Origen from ( select pc_clienteid AS ClienteID, c.Nombre AS ClienteNombre, c.IdiomaID AS ClienteIDiomaID, CASE WHEN pc_fechadocreemplazado IS NOT NULL THEN CONVERT(char(10), pc_fechadocreemplazado, 103) ELSE CONVERT(char(10), pc_fechageneracion, 103) END AS FechaDoc, CASE WHEN pc_fechadocreemplazado IS NOT NULL THEN pc_fechadocreemplazado ELSE pc_fechageneracion END AS FechaDocAsDate, dbo.GetDocumentoNroEscalar(pc_presupuestocabid) AS NroDoc,  pc_saldo AS Saldo, m.AbrevMoneda AS Moneda, pc_origen AS Origen from pc_presupuestocab  join Cliente c   	on c.ID = pc_clienteid  join Moneda m   	on m.ID = pc_monedaid  join Tramite t  on t.ID = pc_tramiteid  where pc_saldo &gt; 0  and pc_estado = 'A' and pc_clienteid = 5282and pc_string1 in ( select pc_string1 from pc_presupuestocab where pc_saldo &gt; 0 and pc_estado = 'A' and pc_clienteid =  5282 and COALESCE(pc_string1,'') &lt;&gt; ''group by pc_string1 having count(*) &gt; 1 ) ) AS lpr group by lpr.ClienteID, lpr.ClienteNombre, lpr.ClienteIdiomaID, lpr.FechaDoc, lpr.FechaDocAsDate, lpr.NroDoc, lpr.Moneda, lpr.Origen union select c.ID AS ClienteID, c.Nombre AS ClienteNombre, c.IdiomaID AS ClienteIDiomaID, CASE WHEN c.IdiomaID = 2 THEN t.EtiquetaEsp + COALESCE(pc_concepto, '') ELSE t.EtiquetaIng +  COALESCE(pc_concepto, '') END AS Tramite, pc_string2 AS CodigoRESI, CASE WHEN pc_fechadocreemplazado IS NOT NULL THEN CONVERT(char(10), pc_fechadocreemplazado, 103) ELSE CONVERT(char(10), pc_fechageneracion, 103) END AS FechaDoc, CASE WHEN pc_fechadocreemplazado IS NOT NULL THEN pc_fechadocreemplazado ELSE pc_fechageneracion END AS FechaDocAsDate, dbo.GetDocumentoNroEscalar(pc_presupuestocabid) AS NroDoc,  pc_saldo AS Saldo, m.AbrevMoneda AS Moneda, pc_fechageneracion AS FechaGeneracion, pc_presupuestocabid AS PresupuestoID, pc_tramiteid AS TramiteID, pc_origen AS Origen from pc_presupuestocab  join Cliente c   on c.ID = pc_clienteid  join Moneda m   on m.ID = pc_monedaid  join Tramite t  on t.ID = pc_tramiteid  where pc_saldo &gt; 0  and pc_estado = 'A' and pc_clienteid = 5282 and (pc_string1 not in ( select pc_string1 from pc_presupuestocab where pc_saldo &gt; 0  and pc_estado = 'A' and pc_clienteid = 5282 and COALESCE(pc_string1,'') &lt;&gt; ''group by pc_string1 having count(*) &gt; 1 ) or COALESCE(pc_string1,'') = '') UNION SELECT ncp_clienteid, c.Nombre, c.IdiomaID, ncp_cuerponota + ' ' + ncp_presupuestos, NULL, CONVERT(char(10), ncp_fecha, 103), ncp_fecha, CAST(ncp_nrocomprobante AS varchar), ncp_saldo * -1,  m.AbrevMoneda, CONVERT(char(10), ncp_fecha, 103), -1, -1, 'X' FROM ncp_notacreditopresup JOIN Cliente c 	ON ncp_clienteid = c.ID JOIN Moneda m 	ON ncp_monedaid = m.ID WHERE ncp_saldo &gt; 0 AND ncp_anulado = 0 AND ncp_clienteid = 5282order by FechaDocAsDate ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
